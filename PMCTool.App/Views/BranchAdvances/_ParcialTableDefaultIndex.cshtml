@model IEnumerable<PMCTool.Models.Environment.ReportBrachAdvances>; 

 @if (Model != null)
 {
     <div class="fresh-datatables">
                                     <table id="tablebrachadvances" class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
                                        <thead>
                                                    <tr>
                                                        <th>Sucursales</th> 
                                                        <th  class="text-center">Obra civil</th>
                                                        <th  class="text-center">Legal</th>
                                                        <th  class="text-center">Recursos Humanos</th>
                                                        <th  class="text-center">Tecnología</th>
                                                        <th class="text-center">Puesta en marcha</th> 
                                                    </tr>
                                                </thead> 
                                        <tbody>
                                            @{
                                                @foreach (var row in Model)  
                                                {  
                                                    string txtCivilWork = null;
                                                    int indexCivilWork = 0;
                                                    if (row.CivilWork >= 100)
                                                    { 
                                                        txtCivilWork = "<i class='fas fa-check-circle fa-lg color-gray'>  </i>";
                                                        indexCivilWork = 1;
                            
                                                    }
                                                    if (row.CivilWork < 100)
                                                    {
                                                        txtCivilWork = "<i class='fas fa-minus-circle fa-lg color-orange'>  </i>";
                                                        indexCivilWork = 0;
                                                    }    
                                                     
                                                    string txtLegal = null; 
                                                    int indexLegal = 0;
                                                    if (row.Legal >= 100)
                                                    { 
                                                        txtLegal = "<i class='fas fa-check-circle fa-lg color-gray'>  </i>";
                                                        indexLegal = 1;
                            
                                                    }
                                                    if (row.Legal < 100)
                                                    {
                                                        txtLegal = "<i class='fas fa-minus-circle fa-lg color-orange'>  </i>";
                                                        indexLegal = 0;
                                                    } 
                                                    
                                                    string txtHumanResources = null;
                                                    int indexHumanResources = 0;
                                                    if (row.HumanResources >= 100)
                                                    { 
                                                        txtHumanResources = "<i class='fas fa-check-circle fa-lg color-gray'>  </i>";
                                                        indexHumanResources = 1;
                            
                                                    }
                                                    if (row.HumanResources < 100)
                                                    {
                                                        txtHumanResources = "<i class='fas fa-minus-circle fa-lg color-orange'>  </i>";
                                                        indexHumanResources = 0;
                                                    }   
                                                    
                                                    string txtTechnology = null;
                                                    int indexTechnology = 0;
                                                    if (row.Technology >= 100)
                                                    { 
                                                        txtTechnology = "<i class='fas fa-check-circle fa-lg color-gray'>  </i>";
                                                        indexTechnology = 1;
                            
                                                    }
                                                    if (row.Technology < 100)
                                                    {
                                                        txtTechnology = "<i class='fas fa-minus-circle fa-lg color-orange'>  </i>";
                                                        indexTechnology = 0;
                                                    }                                                            
                                                        
                                                    string txtPreTest = null;
                                                    int indexPreTest = 0;
                                                    if (row.PreTest >= 100)
                                                    { 
                                                        txtPreTest = "<i class='fas fa-check-circle fa-lg color-gray'>  </i>";
                                                        indexPreTest = 1;
                            
                                                    }
                                                    if (row.PreTest < 100)
                                                    {
                                                        txtPreTest = "<i class='fas fa-minus-circle fa-lg color-orange'>  </i>";
                                                        indexPreTest = 0;
                                                    }
                                                         <tr>
                                                            <td><a href="/FactSheet/Index?projectId=@row.ProjectID">@row.ProjectName</a></td>
                                                             <td class="text-center">
                                                                 @Html.Raw(@txtCivilWork) <span style="display:none">@indexCivilWork</span>
                                                             </td>

                                                            <td class="text-center">
                                                                 @Html.Raw(@txtLegal) <span style="display:none">@indexLegal</span>
                                                            </td>
                                                            <td class="text-center">
                                                                 @Html.Raw(@txtHumanResources)  <span style="display:none">@indexHumanResources</span>
                                                            </td>
                                                            <td class="text-center">
                                                                @Html.Raw(@txtTechnology)  <span style="display:none">@indexTechnology</span>
                                                            </td>
                                                            <td class="text-center">
                                                                @Html.Raw(@txtPreTest) <span style="display:none">@indexPreTest</span>
                                                            </td>
                                                         </tr>    
        
                                                 } 
                                            }
                                         
                                        </tbody>
                                    </table>
   </div>
   <script>
       vBranchs.iniDataTable()
   </script>
 }
 else
 {
        <h5 class="color-font-global">No se encontraron resultados para los filtros ingresados</h5>
}

                                        
                                         
                                       