<div class="modal fade" id="licenseModal" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <div class="card horizontal-form" style="border:none;">
                    <div class="card-header ">
                        <h4 id="lmName" class="card-title"></h4>
                        <p id="lmEmail" class="card-category"></p>
                        <br />
                    </div>
                    <div id="dvAssignation" class="card-body ">
                        <div id="lmSelect" class="form-group">
                            <label>@Localizer["LicenseSelect"]</label>
                            @*<select id="selectLicense" name="selectLicense" class="selectpicker" data-title="" data-style="btn-default btn-outline" data-menu-style="dropdown-blue"></select>*@
                            <select id="selectLicense" name="selectLicense" class="form-control"></select>
                        </div>
                        <div id="lmNoLicense" class="form-group text-center">
                            <h4>@Localizer["LicenseNoAvailable"]</h4>
                        </div>
                    </div>
                    <div id="dvRemove" class="card-body ">
                        <div class="form-group text-center">
                            <h4 id="lmLicenseName"></h4>
                        </div>
                        <div class="form-group text-center">
                            <button type="button" onclick="removeLicense(); return false;" class="btn btn-default btn-fill">@Localizer["Remove"] @Localizer["License"]</button>
                            <button type="button" onclick="selectLicense(); return false;" class="btn btn-default btn-fill">@Localizer["Update"] @Localizer["License"]</button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 text-right">
                        <button type="button" class="btn btn-info btn-fill" data-dismiss="modal">@Localizer["Cancel"]</button>
                        <button type="button" id="btnUpdate" onclick="updateLicense(); return false;" class="btn btn-default btn-fill">@Localizer["Update"]</button>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>

