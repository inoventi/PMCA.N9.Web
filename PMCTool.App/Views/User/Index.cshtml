            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="connectivityModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <div class="card horizontal-form" style="border:none;">
                    <div class="card-header ">
                        <h5 class="card-title">@Localizer["Connectivity"]</h5>
                    </div>
                    <div class="card-body ">
                        <form id="frmConnectivity">
                            <div class="form-group">
                                <label>@Localizer["IPAddress"]</label>
                                <input type="text" placeholder="" class="form-control" id="ipaddress" name="ipaddress" maxlength="15" required>
                            </div>
                            <div class="card-footer text-right">
                                <button type="button" id="btnWithoutRestriction" onclick="saveUserConnection(); return false;" class="btn btn-default btn-fill">@Localizer["WithoutRestriction"]</button>
                                <button type="button" id="btnAddIP" onclick="createUserConnection(); return false;" class="btn btn-default btn-fill">@Localizer["Enter"] @Localizer["IPAddress"]</button>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="card data-tables">
                            <div class="card-body table-striped table-no-bordered table-hover dataTable dtr-inline table-full-width">
                                <div class="toolbar">
                                    <!--        Here you can write extra buttons/actions for the toolbar          -->
                                </div>
                                <div class="fresh-datatables">
                                    <table id="datatablesIP" class="table table-striped table-no-bordered table-hover display responsive nowrap" cellspacing="0" width="100%" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th></th>
                                                <th>@Localizer["IPAddress"]</th>
                                                <th class="disabled-sorting text-right">@Localizer["Action"]</th>
                                            </tr>
                                        </thead>
                                        <tfoot>
                                            <tr>
                                                <th></th>
                                                <th>@Localizer["IPAddress"]</th>
                                                <th class="disabled-sorting text-right">@Localizer["Action"]</th>
                                            </tr>
                                        </tfoot>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 text-right">
                        <button type="button" class="btn btn-info btn-fill" data-dismiss="modal">@Localizer["Close"]</button>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>

